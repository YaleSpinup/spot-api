{ 
  "listenAddress": ":8080",
  "accounts": {
    "sandbox": {
      "region": "us-east-1",
      "id": "{{ .sandbox_account_id }}",
      "token": "{{ .sandbox_token }}"
    },
    "spinup": {
      "region": "us-east-1",
      "id": "{{ .spinup_account_id }}",
      "token": "{{ .spinup_token }}"
    },
    "spinupsec": {
      "region": "us-east-1",
      "id": "{{ .spinupsec_account_id }}",
      "token": "{{ .spinupsec_token }}"
    }
  },
  "token": "{{ .api_token }}",
  "logLevel": "{{ .log_level }}",
  "org": "{{ .spinup_org }}"
}
